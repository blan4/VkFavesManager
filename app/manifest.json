{
  "name": "VkFavesManager",
  "description": "Manage your faves from vk.com",
  "version": "1",
  "manifest_version": 2,
  "default_locale": "en",
  "permissions": [
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "scripts/main.js",
        "scripts/chromereload.js"
      ]
    }
  }
}
